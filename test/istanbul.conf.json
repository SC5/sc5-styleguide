{
  "verbose": false,
  "instrumentation": {
    "root": "",
    "default-excludes": true,
    "excludes": ["lib/app/js/components"],
    "embed-source": false,
    "variable": "__coverage__",
    "compact": true,
    "preserve-comments": false,
    "complete-copy": false,
    "save-baseline": false,
    "baseline-file": "coverage/coverage-baseline.json",
    "preload-sources": false
  },
  "reporting": {
    "print": "summary",
    "reports": ["json"],
    "dir": "./coverage",
    "watermarks": {
      "statements": [50, 80],
      "lines": [50, 80],
      "functions": [50, 80],
      "branches": [50, 80]
    },
    "report-config": {
      "clover": { "file": "clover.xml" },
      "cobertura": { "file": "cobertura-coverage.xml" },
      "json": { "file": "functional-coverage.json" },
      "json-summary": { "file": "coverage-summary.json" },
      "lcovonly": { "file": "lcov.info" },
      "teamcity": { "file": null},
      "text": {
        "file": null,
        "maxCols": 0
      },
      "text-summary": { "file": null }
    }
  },
  "hooks": {
    "hook-run-in-context": false,
    "post-require-hook": null,
    "handle-sigint": false
  }
}